{
  "dashboard": {
    "id": null,
    "title": "Spring Boot + Apache Pekko - Stocks Service",
    "tags": ["spring-boot", "pekko", "stocks", "microservices"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Application Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"stocks-app\"}",
            "legendFormat": "Health Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "color": "red",
                    "text": "DOWN"
                  },
                  "1": {
                    "color": "green",
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
