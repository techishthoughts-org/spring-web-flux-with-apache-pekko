            "expr": "jvm_memory_used_bytes{job=\"stocks-app\", area=\"heap\"}",
            "legendFormat": "{{id}}",
            "refId": "A"
          },
          {
            "expr": "jvm_memory_max_bytes{job=\"stocks-app\", area=\"heap\"}",
            "legendFormat": "{{id}} (max)",
            "refId": "B"
